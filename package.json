{
  "name": "kong-services-api",
  "version": "1.0.0",
  "description": "Kong Services API",
  "author": "Danielle Havimaki",
  "license": "MIT",
  "engines": {
    "node": "14.17.0"
  },
  "scripts": {
    "local:up": "docker-compose -f docker-compose.yml up",
    "local:upbuild": "docker-compose -f docker-compose.yml up --build",
    "local:rebuild": "docker-compose -f docker-compose.yml down && docker-compose -f docker-compose.yml up --build",
    "test:upbuild": "docker-compose -f docker-compose.test.yml up --build",
    "test:rebuild": "docker-compose -f docker-compose.test.yml down && docker-compose -f docker-compose.test.yml up --build",
    "start:dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/typeorm": "^6.3.4",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "typeorm": "^0.2.45",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/node": "18.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "sinon": "^14.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.10"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "src/service/service.service.spec.ts",
      "src/auth/auth.service.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
